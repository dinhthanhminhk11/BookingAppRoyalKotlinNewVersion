<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph.xml"
    app:startDestination="@id/kingMainFragment">

    <fragment
        android:id="@+id/kingMainFragment"
        android:name="com.example.bookingapproyalkotlinver3.ui.fragment.KingMainFragment"
        android:label="fragment_king_main"
        tools:layout="@layout/fragment_king_main">
        <action
            android:id="@+id/action_kingMainFragment_to_detailHotelFragment"
            app:destination="@id/detailHotelFragment"
            app:enterAnim="@anim/fragment_slide_left_enter"
            app:exitAnim="@anim/fragment_slide_left_exit"
            app:popEnterAnim="@anim/fragment_slide_right_enter"
            app:popExitAnim="@anim/fragment_slide_right_exit" />
        <action
            android:id="@+id/action_kingMainFragment_to_loginFragment"
            app:destination="@id/loginFragment"
            app:enterAnim="@anim/fragment_slide_left_enter"
            app:exitAnim="@anim/fragment_slide_left_exit"
            app:popEnterAnim="@anim/fragment_slide_right_enter"
            app:popExitAnim="@anim/fragment_slide_right_exit" />
        <action
            android:id="@+id/action_kingMainFragment_to_registerFragment"
            app:destination="@id/registerFragment"
            app:enterAnim="@anim/fragment_slide_left_enter"
            app:exitAnim="@anim/fragment_slide_left_exit"
            app:popEnterAnim="@anim/fragment_slide_right_enter"
            app:popExitAnim="@anim/fragment_slide_right_exit" />
        <action
            android:id="@+id/action_kingMainFragment_to_aboutUsFragment"
            app:destination="@id/aboutUsFragment"
            app:enterAnim="@anim/fragment_slide_left_enter"
            app:exitAnim="@anim/fragment_slide_left_exit"
            app:popEnterAnim="@anim/fragment_slide_right_enter"
            app:popExitAnim="@anim/fragment_slide_right_exit" />
        <action
            android:id="@+id/action_kingMainFragment_to_helpFragment"
            app:destination="@id/helpFragment"
            app:enterAnim="@anim/fragment_slide_left_enter"
            app:exitAnim="@anim/fragment_slide_left_exit"
            app:popEnterAnim="@anim/fragment_slide_right_enter"
            app:popExitAnim="@anim/fragment_slide_right_exit" />
    </fragment>
    <fragment
        android:id="@+id/detailHotelFragment"
        android:name="com.example.bookingapproyalkotlinver3.ui.fragment.DetailHotelFragment"
        android:label="fragment_detail_hotel_activity"
        tools:layout="@layout/fragment_detail_hotel_activity" />
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.example.bookingapproyalkotlinver3.ui.fragment.user.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login">
        <action
            android:id="@+id/action_loginFragment_to_registerFragment"
            app:destination="@id/registerFragment"
            app:enterAnim="@anim/fragment_slide_left_enter"
            app:exitAnim="@anim/fragment_slide_left_exit"
            app:popEnterAnim="@anim/fragment_slide_right_enter"
            app:popExitAnim="@anim/fragment_slide_right_exit" />
    </fragment>
    <fragment
        android:id="@+id/registerFragment"
        android:name="com.example.bookingapproyalkotlinver3.ui.fragment.user.RegisterFragment"
        android:label="fragment_register"
        tools:layout="@layout/fragment_register">
        <action
            android:id="@+id/action_registerFragment_to_loginFragment"
            app:destination="@id/loginFragment"
            app:enterAnim="@anim/fragment_slide_left_enter"
            app:exitAnim="@anim/fragment_slide_left_exit"
            app:popEnterAnim="@anim/fragment_slide_right_enter"
            app:popExitAnim="@anim/fragment_slide_right_exit" />
    </fragment>
    <fragment
        android:id="@+id/aboutUsFragment"
        android:name="com.example.bookingapproyalkotlinver3.ui.fragment.setting.AboutUsFragment"
        android:label="fragment_about_us"
        tools:layout="@layout/fragment_about_us" />
    <fragment
        android:id="@+id/helpFragment"
        android:name="com.example.bookingapproyalkotlinver3.ui.fragment.setting.HelpFragment"
        android:label="fragment_help"
        tools:layout="@layout/fragment_help" />
</navigation>